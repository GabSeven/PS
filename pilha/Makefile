CC = gcc
CFLAGS = -Wall -Wextra -g

# Fontes
SRCS = fila_dinamica.c fila_hierarquica.c exPGM2.c
HEADERS = fila_dinamica.h fila_hierarquica.h
OBJS = $(SRCS:.c=.o)

# Nome do execut√°vel
TARGET = teste

all: $(TARGET)

$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) -o $@ $^

%.o: %.c
	$(CC) $(CFLAGS) -c $<

clean:
	rm -f $(OBJS) $(TARGET)